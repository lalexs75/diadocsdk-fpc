<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Torg12SellerTitle">
    <xs:complexType>
      <xs:all>
        <xs:element name="Seller" type="DocflowParticipant">
          <xs:annotation>
            <xs:documentation>Участник документооборота, отпустивший товар</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Buyer" type="DocflowParticipant">
          <xs:annotation>
            <xs:documentation>Участник документооборота, которому отпущен товар</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="Shipper" type="OrganizationDetails_WithAddressOptional">
          <xs:annotation>
            <xs:documentation>Грузоотправитель</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="Consignee" type="OrganizationDetails">
          <xs:annotation>
            <xs:documentation>Грузополучатель</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="Supplier" type="OrganizationDetails">
          <xs:annotation>
            <xs:documentation>Поставщик</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="Payer" type="OrganizationDetails">
          <xs:annotation>
            <xs:documentation>Плательщик</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="Grounds">
          <xs:annotation>
            <xs:documentation>Основание</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:attribute name="DocumentName" type="string255" use="optional" />
            <xs:attribute name="DocumentNumber" type="string256" use="optional" />
            <xs:attribute name="DocumentDate" type="date" use="optional" />
            <xs:attribute name="AdditionalInfo" type="string1000z" use="optional" />
          </xs:complexType>
        </xs:element>
        <xs:element name="Table" type="Torg12Table" />
        <xs:element minOccurs="0" name="SupplyAllowedBy" type="Official">
          <xs:annotation>
            <xs:documentation>Отпуск разрешил</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="SupplyPerformedBy" type="Official">
          <xs:annotation>
            <xs:documentation>Отпуск произвел</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="ChiefAccountant" type="Official">
          <xs:annotation>
            <xs:documentation>Главный бухгалтер</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Signer" type="SignerInfo">
          <xs:annotation>
            <xs:documentation>Подписант</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:all>
      <xs:attribute name="DocumentDate" type="date" use="required">
        <xs:annotation>
          <xs:documentation>Дата товарной накладной</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="DocumentNumber" type="string255" use="required">
        <xs:annotation>
          <xs:documentation>Номер товарной накладной</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="WaybillDate" type="date" use="optional">
        <xs:annotation>
          <xs:documentation>Дата составления транспортной накладной</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="WaybillNumber" type="string256" use="optional">
        <xs:annotation>
          <xs:documentation>Номер транспортной накладной</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="OperationCode" type="string255" use="optional">
        <xs:annotation>
          <xs:documentation>Код вида операции</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Total" use="required">
        <xs:annotation>
          <xs:documentation>Сумма с учетом НДС - всего по накладной</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:totalDigits value="17" />
            <xs:fractionDigits value="2" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="TotalInWords" type="string255" use="optional">
        <xs:annotation>
          <xs:documentation>Сумма с учетом НДС - всего по накладной, прописью</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SupplyDate" type="date" use="optional">
        <xs:annotation>
          <xs:documentation>Дата отпуска</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="AdditionalInfo" type="string2000z" use="optional">
        <xs:annotation>
          <xs:documentation>Дополнительные сведения</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="AttachmentSheetsQuantity" type="integer10" use="optional">
        <xs:annotation>
          <xs:documentation>Приложение, количество листов</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="Torg12Table">
    <xs:annotation>
      <xs:documentation>Табличные сведения товарной накладной</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="Item">
        <xs:complexType>
          <xs:attribute name="Name" type="string1000" use="optional">
            <xs:annotation>
              <xs:documentation>Наименование</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Feature" type="string1000" use="optional">
            <xs:annotation>
              <xs:documentation>Характеристика</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Sort" type="string10" use="optional">
            <xs:annotation>
              <xs:documentation>Сорт товара</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="NomenclatureArticle" type="string50" use="optional">
            <xs:annotation>
              <xs:documentation>Артикул</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Code" type="string100" use="optional">
            <xs:annotation>
              <xs:documentation>Код товара</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="UnitCode" type="okei" use="optional">
            <xs:annotation>
              <xs:documentation>Код единицы измерения</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="UnitName" type="string1000" use="optional">
            <xs:annotation>
              <xs:documentation>Наименование единицы измерения</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ParcelType" type="string1000" use="optional">
            <xs:annotation>
              <xs:documentation>Вид упаковки</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ParcelCapacity" type="integer10" use="optional">
            <xs:annotation>
              <xs:documentation>Количество в одном месте</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ParcelsQuantity" type="integer12" use="optional">
            <xs:annotation>
              <xs:documentation>Количество мест</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="GrossQuantity" use="optional">
            <xs:annotation>
              <xs:documentation>Брутто</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="12" />
                <xs:fractionDigits value="3" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="Quantity" use="required">
            <xs:annotation>
              <xs:documentation> нетто, количество (масса)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="12" />
                <xs:fractionDigits value="3" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="Price" use="optional">
            <xs:annotation>
              <xs:documentation>Цена</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="14" />
                <xs:fractionDigits value="2" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="TaxRate" use="optional">
            <xs:annotation>
              <xs:documentation>Ставка налога</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="10" />
                <xs:fractionDigits value="5" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="SubtotalWithVatExcluded" use="optional">
            <xs:annotation>
              <xs:documentation>Сумма без учета налога</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="17" />
                <xs:fractionDigits value="2" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="Vat" use="optional">
            <xs:annotation>
              <xs:documentation>Сумма налога</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="17" />
                <xs:fractionDigits value="2" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="Subtotal" use="required">
            <xs:annotation>
              <xs:documentation>Сумма всего</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="17" />
                <xs:fractionDigits value="2" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="AdditionalInfo" type="string1000z" use="optional">
            <xs:annotation>
              <xs:documentation>Информационное поле сведений о товаре</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="ParcelsQuantityTotal" type="integer10" use="optional">
      <xs:annotation>
        <xs:documentation>Количество мест, штук - всего по накладной</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ParcelsQuantityTotalInWords" type="string255" use="optional">
      <xs:annotation>
        <xs:documentation>Количество мест, штук - всего по накладной, прописью</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="GrossQuantityTotal" use="optional">
      <xs:annotation>
        <xs:documentation>Брутто - всего по накладной</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="12" />
          <xs:fractionDigits value="3" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="GrossQuantityTotalInWords" type="string255" use="optional">
      <xs:annotation>
        <xs:documentation>Брутто - всего по накладной, прописью</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="NetQuantityTotal" use="optional">
      <xs:annotation>
        <xs:documentation>Нетто - всего по накладной</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="12" />
          <xs:fractionDigits value="3" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="NetQuantityTotalInWords" type="string255" use="optional">
      <xs:annotation>
        <xs:documentation>Нетто - всего по накладной, прописью</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="QuantityTotal" use="optional">
      <xs:annotation>
        <xs:documentation>Количество (масса нетто) - всего по накладной</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="12" />
          <xs:fractionDigits value="3" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="TotalWithVatExcluded" use="optional">
      <xs:annotation>
        <xs:documentation>Сумма без учета НДС - всего по накладной</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="17" />
          <xs:fractionDigits value="2" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Vat" use="optional">
      <xs:annotation>
        <xs:documentation>Сумма НДС - всего по накладной</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="17" />
          <xs:fractionDigits value="2" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="string8">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="8" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string10">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="10" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string14f">
    <xs:restriction base="xs:string">
      <xs:length value="14" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string20">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="20" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string20f">
    <xs:restriction base="xs:string">
      <xs:length value="20" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string24">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="24" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string27f">
    <xs:restriction base="xs:string">
      <xs:length value="27" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string29">
    <xs:restriction base="xs:string">
      <xs:maxLength value="29" />
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string30">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="30" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string36">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="36" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string50">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="50" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string60">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="60" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string100">
    <xs:restriction base="xs:string">
      <xs:maxLength value="100" />
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string100z">
    <xs:restriction base="xs:string">
      <xs:maxLength value="100" />
      <xs:minLength value="0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string120">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="120" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string128">
    <xs:restriction base="xs:string">
      <xs:maxLength value="128" />
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string128z">
    <xs:restriction base="xs:string">
      <xs:maxLength value="128" />
      <xs:minLength value="0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string255">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="255" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string256">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="256" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string1000">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="1000" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string1000z">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="1000" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string2000">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="2000" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="string2000z">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="2000" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="inn">
    <xs:restriction base="xs:string">
      <xs:minLength value="10" />
      <xs:maxLength value="12" />
      <xs:pattern value="([0-9]{1}[1-9]{1}|[1-9]{1}[0-9]{1})([0-9]{10}|[0-9]{8})" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="guid">
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12})|(\{[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}\})" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="okei">
    <xs:annotation>
      <xs:documentation>Код из Общероссийского классификатора единиц измерения</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="3" />
      <xs:maxLength value="4" />
      <xs:pattern value="[0-9]{3}" />
      <xs:pattern value="[0-9]{4}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="okv">
    <xs:annotation>
      <xs:documentation>Код из Общероссийского классификатора валют</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="3" />
      <xs:pattern value="[0-9]{3}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="date">
    <xs:annotation>
      <xs:documentation>Дата в формате ДД.ММ.ГГГГ (01.01.1800 - 31.12.2099)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="10" />
      <xs:pattern value="(((0[1-9]{1}|[1-2]{1}[0-9]{1})\.(0[1-9]{1}|1[0-2]{1}))|((30)\.(01|0[3-9]{1}|1[0-2]{1}))|((31)\.(01|03|05|07|08|10|12)))\.(18[0-9]{2}|19[0-9]{2}|20[0-9]{2})" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="integer10">
    <xs:restriction base="xs:integer">
      <xs:totalDigits value="10" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="integer12">
    <xs:restriction base="xs:integer">
      <xs:totalDigits value="12" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CommonRevisionNumber">
    <xs:restriction base="xs:integer">
      <xs:totalDigits value="3" />
      <xs:minInclusive value="1" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="AdditionalInfoId">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="20" name="AdditionalInfo" type="AdditionalInfo">
        <xs:annotation>
          <xs:documentation>ТекстИнф - Текстовая информация</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="InfoFileId" use="optional">
      <xs:simpleType>
        <xs:annotation>
          <xs:documentation>ИдФайлИнфПол (GUID) - Идентификатор файла информационного поля</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:maxLength value="36" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="AdditionalInfo">
    <xs:attribute name="Id" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="50" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Value" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="2000" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="CustomsDeclaration">
    <xs:attribute name="Country" type="string1000" use="required">
      <xs:annotation>
        <xs:documentation>
					КодПроисх - Код страны происхождения товара по Общероссийскому классификатору стран мира (ОКСМ) или
					980 - Евросоюз,
					981 - ЕАЭС
				</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="DeclarationNumber" type="string1000" use="required">
      <xs:annotation>
        <xs:documentation>Номер таможенной декларации</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="CustomsDeclarationWithHyphens">
    <xs:attribute name="Country" type="string1000" use="optional">
      <xs:annotation>
        <xs:documentation>
					КодПроисх - Код страны происхождения товара по Общероссийскому классификатору стран мира (ОКСМ) или
					980 - Евросоюз,
					981 - ЕАЭС
				</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute default="false" name="HyphenCountry" use="optional">
      <xs:annotation>
